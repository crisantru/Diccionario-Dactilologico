					<h2>Selección</h2>
					<script>
				    function myFunction(name,id) {

				        var x = document.createElement("TABLE");
				        x.setAttribute("id", "myTable");
				        x.setAttribute("class", "table");
				        document.body.appendChild(x);

				        var y = document.createElement("TR");
				        y.setAttribute("id", "myTr");
				        document.getElementById("myTable").appendChild(y);

				        var z = document.createElement("TD");
				        z.setAttribute("id", id);
				        var img = document.createElement("IMG");
				        img.setAttribute("src", name);
				        img.setAttribute("name", name);
				        img.setAttribute("id", id)

				        z.appendChild(img);
				        document.getElementById("myTr").appendChild(z);

				    }

				    function palabra(){

				    var texto = "";
						var rows = document.getElementById("myTable").rows.length;
						var celdas = document.getElementById("myTable").rows[0].cells.length;

						for (i = 0; i < celdas; i++) {
							 texto += document.getElementById("myTable").rows[0].cells[i].id;
						}

						document.getElementById("traduccion").innerHTML = texto;

				    }
	    </script>
