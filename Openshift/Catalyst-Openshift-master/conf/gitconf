    fetch = +refs/heads/*:refs/remotes/origin/*
    url = https://github.com/jillrowe/Catalyst-Openshift.git
[remote "openshift"]
    fetch = +refs/heads/*:refs/remotes/origin/*
    url = ssh://lettersandnumber@test-advcomp.rhcloud.com/~/git/test.git/
[branch "master"]
     remote = origin
     merge = refs/heads/master
[rhc]
    app-uuid = lettersandnumber
    app-name = test
    domain-name = advcomp

